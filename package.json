{
  "name": "enquirer-prompts",
  "description": "Plugin that adds a number of common prompt types to Enquirer",
  "version": "1.0.0",
  "homepage": "https://github.com/enquirer/enquirer-prompts",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "enquirer/enquirer-prompts",
  "bugs": {
    "url": "https://github.com/enquirer/enquirer-prompts/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "prompt-checkbox": "^2.0.1",
    "prompt-confirm": "^1.0.0",
    "prompt-editor": "^0.1.0",
    "prompt-expand": "^1.0.0",
    "prompt-list": "^2.0.0",
    "prompt-password": "^1.0.2",
    "prompt-radio": "^1.0.0",
    "prompt-rawlist": "^2.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "enquirerprompt",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
